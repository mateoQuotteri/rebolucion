<!DOCTYPE html>
<html lang="en">
<head>
    <%-include ("../partials/head") %> 
    <link rel="stylesheet" href="/styles/register.css">
</head>
<body class="">
<main>
    <section class="row fit notFlex">
        <article class="col-md-10 indexFlex ">
            <% units.forEach(unit => {%>
            
                <div class="p-3 justBorderIndex  bg-light tripCard">
                    <div>
            
                        <p  height="fit-content" class="text-card"><%= unit.title %> </p>
                       

                       
                        </button>
                        <p  height="fit-content" 
                        class="text-card">Descripcion de la unidad :<%= unit.description %>/10 </p>
                        <p  height="fit-content" 
                        class="text-card">ID de la unidad :<%= unit.id %></p>
                        <p  height="fit-content" 
                        class="text-card">Pertenece al modulo :<%= unit.id_modulo %> </p>
                    </div>
                </div>
            <% }); %>
        </article>
    </section>
</main>
</body>
</html>